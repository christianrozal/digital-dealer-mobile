{
  "expo": {
    "name": "digital-dealer",
    "slug": "digital-dealer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/alexium-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/alexium-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alexium.digitaldealer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/alexium-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/alexium-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c80bd55b-3ca5-4e3a-a998-f575c76e43c9"
      }
    }
  }
}
